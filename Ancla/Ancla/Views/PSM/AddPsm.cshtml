@model Ancla.Models.PSM

@{
    ViewData["Title"] = "Peptide Spectrum Match";
}

<h4>PSM</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="AddPsm">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="mb-3 mt-3">
                <form method="post">
                    <div class="mb-3">
                        <label class="form-label">Upload File</label>
                        <input type="file" name="file" class="form-control"/>
                    </div>
                </form>
            </div>
            <div class="form-group">
                <button type="submit" asp-action="AddPsm" class="btn btn-primary">Submit</button>
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
